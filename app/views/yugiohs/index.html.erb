<p id="notice"><%= notice %></p>

<%= page_HCMW "Yugioh" %>

<h1>Yugiohs</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @yugiohs.each do |yugioh| %>
      <tr>
        <td><%= link_to 'Show', yugioh %></td>
        <td><%= link_to 'Edit', edit_yugioh_path(yugioh) %></td>
        <td><%= link_to 'Destroy', yugioh, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Yugioh', new_yugioh_path %>
